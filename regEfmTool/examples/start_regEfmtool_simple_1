# with gene regulatory information
cd /autofs/netapp/www/jsansen/regEfmTool/examples && ls && java -Xmx1G -jar ../regEfmtool.jar -log console -level FINEST -format plain -kind stoichiometry -stoich simple_1/example.sfile -rev simple_1/example.rvfile -meta simple_1/example.mfile -reac simple_1/example.rfile -out text-doubles simple_1/modes_inc_generules.text -normalize none -maxthreads 2 -generule simple_1/example.grfile 

